COMPINT = CodeSeg.c.o compiler.c.o interpreter.c.o 	scanner.c.o compint_globals.c.o symbol_table.c.o read_num.c.o	COMPINT_DEFS = instr_codes.h compint.hMAIN = main.c.oCOptions = -d DEBUG -i ::includes:compint  {COMPINT} {MAIN} {COMPINT_DEFS}		Link -d -map -c 'MPS ' -t MPST 		{COMPINT} {MAIN} 		"{CLibraries}"CRuntime.o 		"{Libraries}"Interface.o 		"{CLibraries}"StdCLib.o 		"{CLibraries}"CSANELib.o 		"{CLibraries}"Math.o 		"{CLibraries}"CInterface.o 		"{Libraries}"ToolLibs.o 		-o compint > linkmap		echo -n 'compint '{COMPINT}  {COMPINT_DEFS}